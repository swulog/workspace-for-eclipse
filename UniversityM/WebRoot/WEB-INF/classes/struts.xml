<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	<constant name="struts.multipart.saveDir" value="/uploadFiles"/> 
	<constant name="struts.custom.i18n.resources" value="msg" />
	<!-- 管理后台  -->	
	<package name="system" namespace="/" extends="struts-default">
		<action name="loginSysUser" class="sysUserAction" method="loginSysUser">
			<result name="login_success">/page/success.jsp</result>
			<result name="login_failure">/page/failure.jsp</result>
		</action>
	</package>
</struts>    
